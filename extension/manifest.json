{
  "name": "socket.io Chrome extension",
  "manifest_version": 2,
  "description": "does things",
  "version": "0.0.1",
  "devtools_page": "devtools.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run-at": "document-end"
    }
  ],
  "web_accessible_resources": [
    "script/*"
  ]
}
